<script>
    function isHidden(oDiv) {
        var vDiv = document.getElementById("div" + oDiv);
        var vLab = document.getElementById("lab" + oDiv);
        vDiv.style.display = (vDiv.style.display == 'none') ? 'block' : 'none';
        vLab.innerText = (vDiv.style.display == 'none') ? '+' : '—';
    }
</script>

